        type="email"
        id="inputEmail"
        name="inputEmail"
        class="form-control"
        placeholder="Email address"
        required
        autofocus
      />
      <label for="inputPassword" class="sr-only">Password</label>
      <input
        type="password"
        id="inputPassword"
        name="inputPassword"
        class="form-control"
        placeholder="Password"
        required
      />
